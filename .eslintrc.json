{
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended",
      "prettier/flowtype",
      "prettier/react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest/globals": true
    },
    "plugins": [
        "react",
        "jest",
        "jsx-a11y",
        "import",
      "prettier"
    ],
    "globals": {
        "graphql": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
}